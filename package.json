{
  "name": "chomskei",
  "version": "1.0.0",
  "main": ".eslintrc.js",
  "directories": {
    "doc": "docs",
    "lib": "lib"
  },
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "init-records": "ts-node lib/init_records.ts",
    "deploy": "tsc && cdk deploy",
    "chomskei-local": "ts-node src/index_local.ts",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "format": "npm run lint:fix && prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "format-output-html": "prettier --write out/html/${npm_config_date}"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cdate": "^0.0.6",
    "diff-dom": "^5.0.4",
    "install": "^0.13.0",
    "npm": "^9.7.1",
    "puppeteer-core": "20.6.0",
    "rotating-file-stream": "^3.1.0",
    "ts-dotenv": "^0.9.1",
    "tslog": "^4.8.2",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.350.0",
    "@aws-sdk/client-sns": "^3.360.0",
    "@aws-sdk/credential-providers": "^3.351.0",
    "@kintone/rest-api-client": "^4.1.0",
    "@sparticuz/chromium": "114.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "aws-cdk": "2.83.1",
    "aws-cdk-lib": "2.83.1",
    "esbuild": "^0.18.2",
    "eslint": "^8.42.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.3",
    "yarn": "^1.22.19"
  }
}
